repositories:
  # lidar driver
  ROBOTIS-GIT/hls_lfcd_lds_driver:
    type: git
    url: https://github.com/ROBOTIS-GIT/hls_lfcd_lds_driver
    version: ros2

  # C++ kobuki API
  kobuki-base/kobuki_core:
    type: git
    url: git@github.com:kobuki-base/kobuki_core
    version: devel
  kobuki-base/kobuki_ros_interfaces:
    type: git
    url: git@github.com:kobuki-base/kobuki_ros_interfaces
    version: devel
  kobuki-base/kobuki_ros:
    type: git
    url: git@github.com:kobuki-base/kobuki_ros
    version: devel

  # needed for turtlebot_description only
  emersonknapp/turtlebot:
    type: git
    url: git@github.com:emersonknapp/turtlebot
    version: ros2

  # autonomy
  ros-planning/navigation2:
    type: git
    url: https://github.com/ros-planning/navigation2
    version: foxy-devel
  ros2/cartographer_ros:
    type: git
    url: git@github.com:ros2/cartographer_ros
    version: dashing
  ros-perception/perception_pcl:
    type: git
    url: git@github.com:ros-perception/perception_pcl
    version: eloquent-devel

  # custom code
  emersonknapp/robot_stack:
    type: git
    url: git@github.com:emersonknapp/robot_stack
    version: master
  emersonknapp/visualization_tutorials:
    type: git
    url: git@github.com:emersonknapp/visualization_tutorials
    version: imu-display
